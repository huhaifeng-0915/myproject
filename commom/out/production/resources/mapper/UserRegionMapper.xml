<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hhf.common.user.mapper.UserRegionMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.hhf.common.user.model.UserRegion">
        <id column="ID" property="id" />
    <result column="OPERATE_VERSION" property="operateVersion" />
    <result column="CREATED_BY" property="createdBy" />
    <result column="UPDATED_BY" property="updatedBy" />
    <result column="UPDATED_ON" property="updatedOn" />
    <result column="CREATED_ON" property="createdOn" />
        <result column="USER_LOGIN_CODE" property="userLoginCode" />
        <result column="REGION_CODE" property="regionCode" />
        <result column="REGION _NAME" property="regionName" />
        <result column="LEVEL" property="level" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        OPERATE_VERSION,
        CREATED_BY,
        UPDATED_BY,
        UPDATED_ON,
        CREATED_ON,
        ID, USER_LOGIN_CODE, REGION_CODE, REGION_NAME,LEVEL
    </sql>

</mapper>
